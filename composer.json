{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^2.6",
        "atelierspierrot/famfamfam-silk-sprite": "^1.0",
        "brainbits/fugue-icons-bundle": "^3.5",
        "brick/math": "^0.9.3",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/migrations": "^3.5",
        "doctrine/orm": "^2.12",
        "fortawesome/font-awesome": "^6.1",
        "friendsofsymfony/rest-bundle": "^3.3",
        "friendsofsymfony/user-bundle": "^3.0",
        "guzzlehttp/guzzle": "^7.4",
        "imagine/imagine": "^1.3",
        "incenteev/composer-parameter-handler": "^2.1",
        "knplabs/knp-gaufrette-bundle": "^0.7.2",
        "nelmio/api-doc-bundle": "^4.9",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpseclib/phpseclib": "^3.0",
        "phpstan/phpdoc-parser": "^1.6",
        "predis/predis": "^2.0",
        "ramsey/uuid": "^4.2",
        "sensio/framework-extra-bundle": "^6.2",
        "snc/redis-bundle": "^4.2",
        "sonata-project/exporter": "^2.13",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.17|^2",
        "symfony/framework-bundle": "*",
        "symfony/intl": "5.4.*",
        "symfony/ldap": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/runtime": "*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/webpack-encore-bundle": "^1.14",
        "symfony/yaml": "*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^3.4"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
